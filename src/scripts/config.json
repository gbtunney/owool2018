{
	"typekit":"'https://use.typekit.net/kxg6qeb.css'",
	"render_component_themes": {
		"light":"Light",
		"dark": "Dark",
		"grid-helper":"Grid Helper"
	},
	"baseColors": {
		"white": "#fff",
		"black": "#000",
		"dark-gray": "#585858",
		"tana": "#bdba9c",
		"khaki": "#f0e68c",
		"west-coast": "#64551f",
		"highball": "#90813b",
		"sandstone": "#7b6d5c",
		"schooner": "#8b8378",
		"spice": "#66543e",
		"madras": "#4c3b25",
		"onion": "#33250d",
		"woodrush": "#332915",
		"jacko-bean": "#2e1c08",
		"acadia": "#1e1205",
		"crowshead": "#1a130a",
		"silverleaf": "#71b799",
		"fuscous_gray":"#555",
		"lightning_yellow": "#eec517"
	},
	"schemes": [
		{
			"key": "light",
			"description": "'The main light on dark theme.'",
			"parent": false,
			"colors": {
				"foreground": "fuscous_gray",
				"background": "white",
				"accent": "silverleaf",
				"border": "silverleaf",
				"link": "silverleaf"
			}
		},
		{
			"key": "dark",
			"description": "'DARK THEME!!'",
			"parent": false,
			"colors": {
				"foreground": "white",
				"background": "silverleaf",
				"accent": "lightning_yellow",
				"border": "lightning_yellow",
				"link":"lightning_yellow"
			}
		},
		{
			"key": "top-menu",
			"description": "'top menu!!'",
			"parent": "dark",
			"colors": {
				"background": "jacko-bean"
			}
		},
		{
			"key": "form",
			"description": "'top menu!!'",
			"parent": "dark",
			"colors": {
				"border": "sandstone",
				"background": "madras"
			}
		},
		{
			"key": "tabs",
			"description": "'tabs menu!!'",
			"parent": "dark",
			"colors": {
				"inactive": "onion"
			}
		},
		{
			"key": "grid-helper",
			"description": "'grid helper menu!!'",
			"parent": "dark",
			"colors": {
				"foreground": "white",
				"background": "madras",
				"accent": "blue",
				"border": "red"
			}
		},
		{
			"key": "modal",
			"description": "'grid helper menu!!'",
			"parent": "dark",
			"colors": {
				"overlay": "madras",
				"background": "dark-gray"
			}
		}
	],
	"typography": {
		"required-props": "'font-family, letter-spacing, word-spacing, text-transform, text-decoration, font-style, font-weight'",
		"line-height": 1.3,
		"modularscale": {
			"base": "20px",
			"ratio": 1.2,
			"sm": {
				"base": "25px",
				"ratio": 1.2
			}
		},
		"fonts": {
			"default": {
				"font-family": "monospace",
				"text-transform": "none",
				"text-decoration": "none",
				"letter-spacing": 0,
				"word-spacing": "-1px",
				"font-weight": "normal",
				"font-style": "normal"
			},
			"font-san-serif": {
				"font-family": "cronos-pro-display",
				"letter-spacing":"-1px",
				"title": "'Primary: Mr Eaves Sans'"
			},
			"font-serif": {
				"font-family": "adobe-garamond-pro",
				"title": "'Secondary, Headers : Mrs Eaves'"
			},
			"font-small-caps": {
				"font-family": "adobe-garamond-pro",
				"text-transform": "uppercase",
				"title": "'Accent, Small Caps : Mrs Eaves Roman Small Caps'"
			}
		}
	},
	"slicer": {
		"breakpoints": "0 480px 768px 992px 1200px",
		"breakpoint-names": "xs sm md lg"
	}
}